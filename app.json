{
  "expo": {
    "name": "TwilingLocation",
    "slug": "TwilingLocation",
    "version": "1.0.0",
    "extra": {
      "mapBoxAccessToken": "${process.env.EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN}"
    },
    "orientation": ["portrait", "landscape"],
    "icon": "./assets/images/cargpsalfa.png",
    "scheme": "twiling-location-app-scheme",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "minSdkVersion": 29,
      "targetSdkVersion": 29,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cargpsalfa.png",
        "backgroundColor": "#d7d5d5"
      },
      "package": "ru.twiling.location"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/cargpsalfa.png",
          "dark": {
            "image": "./assets/images/cargpsblack.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#30b024",
          "android": {
            "scaleType": "CENTER_CROP",
            "adaptiveIcon": false,
            "backgroundColor": "#000000",
            "image": "./assets/images/cargpsalfa.png"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "${process.env.EXPO_PUBLIC_RN_MAPBOX_MAPS_DOWNLOAD_TOKEN}"
        }
      ]
    ],
    "packagerOpts": {
      "config": "metro.config.cjs"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
